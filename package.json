    "sqlite3": "^3.1.8",
    "style-loader": "^0.17.0",
    "swig": "^1.4.2",
    "universal-analytics": "^0.4.13",
    "vue": "^2.3.3",
    "vue-apollo": "^2.1.0-beta.8",
    "vue-loader": "^12.0.3",
    "vue-router": "^2.5.3",
